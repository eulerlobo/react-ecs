{
  "family": "store-demo-containers",
  "containerDefinitions": [
    {
      "name": "react-ecs",
      "image": "007696430067.dkr.ecr.sa-east-1.amazonaws.com/react-ecs:latest",
      "cpu": 1,
      "memory": 128,
      "portMappings": [
          {
              "containerPort": 3000,
              "hostPort": 3000
          }
      ],
      "essential": true,
      "environment": [
          {
              "name": "NODE_ENV",
              "value": "production"
          }
      ]
    }
  ]
}
